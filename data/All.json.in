{
    "type": "record",
    "name": "All",
    "doc": "Store an rmf file as a single json record.",
    "fields" : [
        {
            "name" : "file",
            "type" :  @RMF_JSON_FILE@
        },
        {
            "name" : "nodes",
            "type" : @RMF_JSON_HIERARCHY@
        },
        {
            "name" : "frames",
            "doc" : "the hierarchy of the frames",
            "type" : @RMF_JSON_FRAME_HIERARCHY@
        },
        {
            "name" : "data",
            "type" : {
                "type": "map",
                "doc" : "categories by name",
                "values" : {
                    "type" : "array",
                    "doc" : "frames by index",
                    "items" : @RMF_JSON_DATA@
                }
            }
        }
    ]
}
